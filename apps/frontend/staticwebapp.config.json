{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://devtinder-backend.blackforest-8f439a36.eastus.azurecontainerapps.io/api/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization"
  }
}
